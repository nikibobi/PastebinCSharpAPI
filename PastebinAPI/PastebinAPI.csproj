    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Borislav Kosharov</Authors>
    <Company />
    <Description>Simple and easy to use API wrapper for Pastebin</Description>
    <NeutralLanguage />
