    <PackageProjectUrl>https://github.com/nikibobi/pastebin-csharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nikibobi/pastebin-csharp</RepositoryUrl>
    <Copyright>Copyright 2020</Copyright>
    <PackageTags>api wrapper pastebin</PackageTags>
    <Version>1.1.1</Version>
  </PropertyGroup>

</Project>
